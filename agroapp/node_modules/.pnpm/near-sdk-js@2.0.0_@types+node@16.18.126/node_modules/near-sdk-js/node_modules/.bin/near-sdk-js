#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/raihanmd/Documents/blockchain-projects/web3-learning/agroapp/node_modules/.pnpm/near-sdk-js@2.0.0_@types+node@16.18.126/node_modules/near-sdk-js/lib/cli/node_modules:/home/raihanmd/Documents/blockchain-projects/web3-learning/agroapp/node_modules/.pnpm/near-sdk-js@2.0.0_@types+node@16.18.126/node_modules/near-sdk-js/lib/node_modules:/home/raihanmd/Documents/blockchain-projects/web3-learning/agroapp/node_modules/.pnpm/near-sdk-js@2.0.0_@types+node@16.18.126/node_modules/near-sdk-js/node_modules:/home/raihanmd/Documents/blockchain-projects/web3-learning/agroapp/node_modules/.pnpm/near-sdk-js@2.0.0_@types+node@16.18.126/node_modules:/home/raihanmd/Documents/blockchain-projects/web3-learning/agroapp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/raihanmd/Documents/blockchain-projects/web3-learning/agroapp/node_modules/.pnpm/near-sdk-js@2.0.0_@types+node@16.18.126/node_modules/near-sdk-js/lib/cli/node_modules:/home/raihanmd/Documents/blockchain-projects/web3-learning/agroapp/node_modules/.pnpm/near-sdk-js@2.0.0_@types+node@16.18.126/node_modules/near-sdk-js/lib/node_modules:/home/raihanmd/Documents/blockchain-projects/web3-learning/agroapp/node_modules/.pnpm/near-sdk-js@2.0.0_@types+node@16.18.126/node_modules/near-sdk-js/node_modules:/home/raihanmd/Documents/blockchain-projects/web3-learning/agroapp/node_modules/.pnpm/near-sdk-js@2.0.0_@types+node@16.18.126/node_modules:/home/raihanmd/Documents/blockchain-projects/web3-learning/agroapp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/cli/cli.js" "$@"
else
  exec node  "$basedir/../../lib/cli/cli.js" "$@"
fi
