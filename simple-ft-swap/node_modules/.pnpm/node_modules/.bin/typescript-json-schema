#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/raihanmd/Documents/blockchain-projects/web3-learning/simple-ft-swap/node_modules/.pnpm/near-typescript-json-schema@0.55.0/node_modules/near-typescript-json-schema/bin/node_modules:/home/raihanmd/Documents/blockchain-projects/web3-learning/simple-ft-swap/node_modules/.pnpm/near-typescript-json-schema@0.55.0/node_modules/near-typescript-json-schema/node_modules:/home/raihanmd/Documents/blockchain-projects/web3-learning/simple-ft-swap/node_modules/.pnpm/near-typescript-json-schema@0.55.0/node_modules:/home/raihanmd/Documents/blockchain-projects/web3-learning/simple-ft-swap/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/raihanmd/Documents/blockchain-projects/web3-learning/simple-ft-swap/node_modules/.pnpm/near-typescript-json-schema@0.55.0/node_modules/near-typescript-json-schema/bin/node_modules:/home/raihanmd/Documents/blockchain-projects/web3-learning/simple-ft-swap/node_modules/.pnpm/near-typescript-json-schema@0.55.0/node_modules/near-typescript-json-schema/node_modules:/home/raihanmd/Documents/blockchain-projects/web3-learning/simple-ft-swap/node_modules/.pnpm/near-typescript-json-schema@0.55.0/node_modules:/home/raihanmd/Documents/blockchain-projects/web3-learning/simple-ft-swap/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../near-typescript-json-schema/bin/typescript-json-schema" "$@"
else
  exec node  "$basedir/../near-typescript-json-schema/bin/typescript-json-schema" "$@"
fi
